  keywords = {coronavirus,Covid-19,data-driven methods,government measures,machine learning,open data,SARS-CoV-2,seasonal behavior},
  file = {/home/skynet3/Zotero/storage/PT36J5ER/Alamo et al. - 2020 - Covid-19 Open-Data Resources for Monitoring, Mode.pdf;/home/skynet3/Zotero/storage/VJ5Q5DEL/htm.html}
