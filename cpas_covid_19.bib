@article{lauSARSTransmissionRisk2004,
  title = {{{SARS Transmission}}, {{Risk Factors}}, and {{Prevention}} in {{Hong Kong}}},
  author = {Lau, Joseph T.F. and Tsui, Hiyi and Lau, Mason and Yang, Xilin},
  date = {2004-04},
  journaltitle = {Emerging Infectious Diseases},
