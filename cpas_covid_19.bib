@dataset{centerforinternationalearthscienceinformationnetwork-ciesin-columbiauniversityGriddedPopulationWorld2018,
  title = {Gridded {{Population}} of the {{World}}, {{Version}} 4 ({{GPWv4}}): {{Basic Demographic Characteristics}}, {{Revision}} 11},
