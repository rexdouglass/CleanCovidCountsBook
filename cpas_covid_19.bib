  pmcid = {PMC7928877},
  keywords = {Antibodies; Viral,Canada,Coronavirus Nucleocapsid Proteins,COVID-19,COVID-19 Serological Testing,Cross-Sectional Studies,Humans,Phosphoproteins,SARS-CoV-2,Seroepidemiologic Studies,serology,seroprevalence,Spike Glycoprotein; Coronavirus},
