}

@article{dharmadhikariSurgicalFaceMasks2012,
  title = {Surgical Face Masks Worn by Patients with Multidrug-Resistant Tuberculosis: Impact on Infectivity of Air on a Hospital Ward},
  shorttitle = {Surgical Face Masks Worn by Patients with Multidrug-Resistant Tuberculosis},
  author = {Dharmadhikari, Ashwin S. and Mphahlele, Matsie and Stoltz, Anton and Venter, Kobus and Mathebula, Rirhandzu and Masotla, Thabiso and Lubbe, Willem and Pagano, Marcello and First, Melvin and Jensen, Paul A. and van der Walt, Martie and Nardell, Edward A.},
  options = {useprefix=true},
