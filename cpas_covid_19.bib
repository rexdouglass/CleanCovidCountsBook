@article{utaziMappingVaccinationCoverage2019,
  title = {Mapping Vaccination Coverage to Explore the Effects of Delivery Mechanisms and Inform Vaccination Strategies},
  author = {Utazi, C. Edson and Thorley, Julia and Alegana, Victor A. and Ferrari, Matthew J. and Takahashi, Saki and Metcalf, C. Jessica E. and Lessler, Justin and Cutts, Felicity T. and Tatem, Andrew J.},
  date = {2019-04-09},
