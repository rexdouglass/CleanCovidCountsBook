@article{wuRiskFactorsSARS2004,
  title = {Risk {{Factors}} for {{SARS}} among {{Persons}} without {{Known Contact}} with {{SARS Patients}}, {{Beijing}}, {{China}}},
  author = {Wu, Jiang and Xu, Fujie and Zhou, Weigong and Feikin, Daniel R. and Lin, Chang-Ying and He, Xiong and Zhu, Zonghan and Liang, Wannian and Chin, Daniel P. and Schuchat, Anne},
